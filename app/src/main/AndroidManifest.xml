<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.spotify.repairit">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LandingActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".ContactActivity"></activity>
        <activity android:name=".DeviceActivity"></activity>
        <activity android:name=".ScheduleRepairActivity"></activity>
        <activity android:name=".SignupActivity"></activity>
        <activity
            android:name=".YourOrdersActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"></activity>
        <activity android:name=".YourOrderDetailsActivity"></activity>
        <activity android:name=".AboutActivity"
            ></activity>
        <activity android:name=".ContactusActivity"
           ></activity>
    </application>

</manifest>